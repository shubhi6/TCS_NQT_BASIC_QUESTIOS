-----------------------------------1-------------------------------

# Maximum and Minimum Digit in a Number
n = int(input())
maxi = float('-inf')
mini = float('inf')
while n>0:
    l = n%10
    if l>= maxi:
        maxi = l
    if l< mini:
        mini = l
    n=n//10
print(maxi,mini)
  
-----------------------------------2-------------------------------

# Print Fibonacci Series up to Nth term
n=int(input())
a,b=0,1
for i in range(n):
    print(a,end=" ")
    a,b=b,a+b   
print()

