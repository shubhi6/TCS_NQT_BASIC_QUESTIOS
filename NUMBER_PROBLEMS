# Greatest of two number
a,b=map(int,input().split())
print(max(a,b))
---------or-----
a,b=map(int,input().split())
if a>b:
    print(a)
elif b>a:
    print(b)
else:
    print("equal")

-----------------------------------10-------------------------------

#Greatest of three number 
a,b,c=map(int,input().split())
print(max(a,b,c))
---------or-----
a,b,c=map(int,input().split())
if a>b and a>c:
    print(a)
elif b>a and b>c:
    print(b)
elif c>a and c>b:
    print(c)
else:
    print("equal")

