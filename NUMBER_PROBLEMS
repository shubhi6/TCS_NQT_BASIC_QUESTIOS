# Factors of a Given Number
n=int(input())
for i in range (1,n+1):
    if n%i==0:
        print(i,end=" ")
    
-----------------------------------5-------------------------------

# Prime in given range
s=int(input())
e=int(input())
for i in range(s,e):
    if i>1:
        for j in range(2,i):
            if(i%j)==0:
                break
        else:
            print(i)

